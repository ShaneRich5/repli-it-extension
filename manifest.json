{
    "manifest_version": 2,
    
    "name": "Gist Runner",
    "description": "Launches a new repl-it session for each gist",
    "version": "1.0",
    
    "browser_action": {
        "default_icon": "replit.png",
        "default_popup": "popup.html",
        "default_title": "Click here!"
    },

    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}